{
  "interaction": {
    "type": "script",
    "path": "hook_stealth.js",
    "on_change": "reload",
    "parameters": {
      "stealth_mode": true,
      "anti_detection": true,
      "random_timing": true
    }
  },
  "teardown": "full",
  "runtime": "v8",
  "stealth": {
    "rename_threads": true,
    "hide_port": true,
    "obfuscate_strings": true,
    "anti_debug": true,
    "random_delays": {
      "enabled": true,
      "min": 1.0,
      "max": 5.0,
      "jitter": 0.5
    },
    "memory_protection": {
      "enabled": true,
      "xor_key": 173,
      "rotate_key": true
    }
  },
  "scheduling": {
    "priority": "low",
    "affinity": "auto"
  },
  "network": {
    "port": 0,
    "bind": "127.0.0.1",
    "randomize_port": true
  },
  "logging": {
    "level": "error",
    "output": "file",
    "file": "Logs/frida_stealth.log",
    "obfuscate": true
  }
}
